xprod <- function(a,b) {
if(length(a)==3 && length(b)==3){
getDet <- function(w,x,y,z) {
return(det(matrix(c(w,x,y,z),ncol=2)))
}
return(c(getDet(a[2],b[2],a[3],b[3]),-getDet(a[1],b[1],a[3],b[3]),getDet(a[1],b[1],a[2],b[2])))
}else{
return(FALSE)
}
}
